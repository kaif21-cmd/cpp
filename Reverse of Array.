#include<iostream>
using namespace std;
int main()
{
    int ar[100],i,n;
    cout<<"Enter the array elements:\n";
    cin>>n;
    cout<<"Enter the number of element one by one\n";
    for(i=0;i<n;i++)
    cin>>ar[i];
    cout<<"\n The original array is:\n";
    for(i=0;i<n;i++)
    cout<<ar[i]<<" ";
    cout<<"\n\n The reverse of given array:\n";
    for(i=(n-1);i>=0;i--)
    cout<<ar[i]<<" ";
    cout<<endl;
}
